{
    "name": "telescope-backend",
    "version": "0.7.0",
    "description": "telescope api server",
    "author": "Beyond_Imagination",
    "license": "Apache-2.0",
    "private": true,
    "scripts": {
        "start": "yarn build && cross-env NODE_ENV=production node dist/server.js",
        "dev": "cross-env NODE_ENV=development nodemon",
        "build": "rm -rf dist && swc src -d dist --source-maps --copy-files",
        "build:tsc": "tsc && tsc-alias",
        "test": "NODE_ENV=test jest --detectOpenHandles",
        "lint": "eslint --ignore-path .gitignore --ext .ts src/",
        "lint:fix": "yarn lint -- --fix",
        "deploy:prod": "yarn build && pm2 start ecosystem.config.js --only prod",
        "deploy:dev": "pm2 start ecosystem.config.js --only dev",
        "reload:prod": "yarn build && pm2 reload ecosystem.config.js --only prod",
        "reload:dev": "pm2 reload ecosystem.config.js --only dev",
        "docker:run": "node dist/server.js"
    },
    "dependencies": {
        "@typegoose/typegoose": "^9.12.1",
        "@types/newrelic": "^9.13.0",
        "axios": "^1.0.0",
        "bcrypt": "^5.1.0",
        "bottleneck": "^2.19.5",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "class-validator-jsonschema": "^3.1.1",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.1",
        "envalid": "^7.3.1",
        "express": "^4.18.1",
        "express-winston": "^4.2.0",
        "helmet": "^5.1.1",
        "hpp": "^0.2.3",
        "jsonpath": "^1.1.1",
        "jsonwebtoken": "^9.0.0",
        "jwk-to-pem": "^2.0.5",
        "lru-cache": "^7.14.1",
        "mongoose": "^6.6.2",
        "mongoose-paginate-v2": "^1.7.1",
        "newrelic": "^10.0.0",
        "reflect-metadata": "^0.1.13",
        "routing-controllers": "^0.9.0",
        "routing-controllers-openapi": "^3.1.0",
        "swagger-ui-express": "^4.5.0",
        "uuidv4": "^6.2.13",
        "winston": "^3.8.1",
        "winston-daily-rotate-file": "^4.7.1"
    },
    "devDependencies": {
        "@swc/cli": "^0.1.57",
        "@swc/core": "^1.2.220",
        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.13",
        "@types/hpp": "^0.2.2",
        "@types/jest": "^28.1.6",
        "@types/node": "^17.0.45",
        "@types/supertest": "^2.0.12",
        "@types/swagger-ui-express": "^4.1.3",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "axios-mock-adapter": "^1.21.2",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.1",
        "jest": "28.1.3",
        "lint-staged": "^13.0.3",
        "mongodb-memory-server": "^8.10.0",
        "node-config": "^0.0.2",
        "node-gyp": "^9.1.0",
        "nodemon": "^2.0.19",
        "pm2": "^5.2.0",
        "prettier": "^2.7.1",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.7",
        "ts-node": "^10.9.1",
        "tsc-alias": "^1.7.0",
        "tsconfig-paths": "^4.0.0",
        "typescript": "^4.7.4"
    }
}
